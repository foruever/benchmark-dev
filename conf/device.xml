<?xml version="1.0" encoding="UTF-8"?>
<config>
	<device id="fengche1" name="fengche1"> <!-- id不可重复，用来标识设备 -->
		<nametags><!-- tags可没有，此时devicename为数的根 -->
			<tag key="prinvice" value="shanxi"></tag>
			<tag key="city" value="taiyuang"></tag>
		</nametags>
		<sensors>
			<sensor name="temperature" sensor-ref-id="t1" begin-time="2017-06-01 00:00:00" end-time="2017-07-01 00:00:00">
				<tags><!-- tags可没有，此时devicename为数的根 -->
					<tag key="location" value="head"></tag>
				</tags>
			</sensor>
			<sensor name="velocity" sensor-ref-id="v1" begin-time="2017-06-01 00:00:00" end-time="2017-07-01 00:00:00"></sensor>
		</sensors>
	</device>
	
	
	<device id="tuolaji1" name="tuolaji1"> <!-- id不可重复，用来标识设备 -->
		<sensors>
			<sensor name="mileage" sensor-ref-id="mileage1" begin-time="2017-06-01 00:00:00" end-time="2017-07-01 00:00:00"></sensor>
		</sensors>
	</device>	
</config>
